{
	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "Launch server debug",
			"program": "${workspaceRoot}/node_modules/.bin/ts-node-dev",
			"args": ["--inspect", "--no-notify", "--respawn", "--project", "tsconfig.json", "main.ts"],
			"protocol": "inspector",
			"internalConsoleOptions": "openOnSessionStart",
			"port": 9229
		}
	]
}
